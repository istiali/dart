  ob.printing();
}

class libraryy
{
  int book_id = 1;
  int book_qty = 2;
  String book_name = "Harry Potter";
  
  void bookdetails(){
    print("Book ID: ${ob.book_id}");
    print("Book Quantity: ${ob.book_qty}");
    print("Book Name: ${ob.book_name}");
  }
  
}

libraryy ob = libraryy();

